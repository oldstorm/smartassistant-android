<?xml version="1.0" encoding="utf-8"?>
<!--圆的样式  selector_thumb.xml 选中与未选中-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <size android:width="@dimen/dp_18" android:height="@dimen/dp_16"/>
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="@color/color_2da3f6" />
        </shape>
    </item>
    <item >
        <shape android:shape="oval">
            <size android:width="@dimen/dp_18" android:height="@dimen/dp_18"/>
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="#DFDFDF" />
        </shape>
    </item>
</selector>
